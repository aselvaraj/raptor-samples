{
    "port": 8081,
    "optimizer": {
        "urlPrefix": "/static",
        "outputDir": "static",
        "fingerprintsEnabled": false,
        "minify": false,
        "bundlingEnabled": false,
        "resolveCssUrls": true,
        "plugins": [
            "optimizer-less",
            "optimizer-dust",
            "optimizer-marko"
        ]
    }
}